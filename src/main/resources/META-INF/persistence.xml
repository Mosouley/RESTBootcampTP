<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
  <persistence-unit name="monPuWeb" transaction-type="RESOURCE_LOCAL">
    <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
    <class>com.bootcamp.entity.IndicateurPerformance</class>
    <class>com.bootcamp.entity.IndicateurQualitatif</class>
    <class>com.bootcamp.entity.IndicateurQuantitatif</class>
    <class>com.bootcamp.entity.Livrable</class>
    <class>com.bootcamp.entity.Programme</class>
    <class>com.bootcamp.entity.Projet</class>
    <class>com.bootcamp.entity.Personne</class>
    <class>com.bootcamp.entity.Beneficiaire</class>
    <class>com.bootcamp.entity.Fournisseur</class>
    <class>com.bootcamp.entity.Bailleur_Programme</class>
    <class>com.bootcamp.entity.Bailleur</class>
    <class>com.bootcamp.entity.Beneficiaire_Programme</class>
    <class>com.bootcamp.entity.Fournisseur_Programme</class>
    <properties>
      <property name="javax.persistence.jdbc.url" value="jdbc:mysql://localhost:3306/testtpjpa?zeroDateTimeBehavior=convertToNull"/>
      <property name="javax.persistence.jdbc.user" value="root"/>
      <property name="javax.persistence.jdbc.driver" value="com.mysql.jdbc.Driver"/>
      <property name="javax.persistence.jdbc.password" value="Admin"/>
      <property name="eclipselink.canonicalmodel.subpackage" value="one"/>
      <property name="javax.persistence.schema-generation.database.action" value="create"/>
    </properties>
  </persistence-unit>
  <persistence-unit name="PostgresPuWeb" transaction-type="RESOURCE_LOCAL">
    <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
    <class>com.bootcamp.entity.IndicateurPerformance</class>
    <class>com.bootcamp.entity.IndicateurQualitatif</class>
    <class>com.bootcamp.entity.IndicateurQuantitatif</class>
    <class>com.bootcamp.entity.Livrable</class>
    <class>com.bootcamp.entity.Programme</class>
    <class>com.bootcamp.entity.Projet</class>
    <class>com.bootcamp.entity.Personne</class>
    <class>com.bootcamp.entity.Beneficiaire</class>
    <class>com.bootcamp.entity.Fournisseur</class>
    <class>com.bootcamp.entity.Bailleur_Programme</class>
    <class>com.bootcamp.entity.Bailleur</class>
    <class>com.bootcamp.entity.Beneficiaire_Programme</class>
    <class>com.bootcamp.entity.Fournisseur_Programme</class>
    <properties>
      <property name="javax.persistence.jdbc.url" value="jdbc:postgresql://localhost:5432/tpJPA"/>
      <property name="javax.persistence.jdbc.user" value="soul"/>
      <property name="javax.persistence.jdbc.driver" value="org.postgresql.Driver"/>
      <property name="javax.persistence.jdbc.password" value="Admin"/>
      <property name="eclipselink.canonicalmodel.subpackage" value="two"/>
      <property name="javax.persistence.schema-generation.database.action" value="none"/>
    </properties>
  </persistence-unit>
</persistence>
